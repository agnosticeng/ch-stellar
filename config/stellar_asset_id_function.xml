<functions>
    <function>
        <name>stellar_asset_id</name>
        <type>executable_pool</type>
        <format>ArrowStream</format>
        <stderr_reaction>log</stderr_reaction>
        <command>ch-stellar function asset-id</command>

        <pool_size>16</pool_size>
        <max_command_execution_time>300</max_command_execution_time>
        <command_read_timeout>180000</command_read_timeout>
        <command_write_timeout>60000</command_write_timeout>

        <argument>
            <name>asset_code</name>
            <type>String</type>
        </argument>
        <argument>
            <name>asset_issuer</name>
            <type>String</type>
        </argument>
        <argument>
            <name>asset_type</name>
            <type>String</type>
        </argument>

        <return_type>UInt64</return_type>
    </function>
</functions>